clr.AddReference("Bonsai.Harp")
from Bonsai.Harp import *

@returns(HarpMessage)
def process(value):
  return HarpMessage.FromSingle(32, MessageType.Write, value.Magnitude, value.Angle)</scr:Script>
            </Expression>
            <Expression xsi:type="MulticastSubject">
              <Name>ArmCommands</Name>
            </Expression>
