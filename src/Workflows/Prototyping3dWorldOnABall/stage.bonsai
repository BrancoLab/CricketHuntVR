<?xml version="1.0" encoding="utf-8"?>
<WorkflowBuilder Version="2.6.3"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xmlns:sys="clr-namespace:System;assembly=mscorlib"
                 xmlns:p1="clr-namespace:CricketVR;assembly=Extensions"
                 xmlns:wie="clr-namespace:Bonsai.Windows.Input;assembly=Bonsai.Windows.Input"
                 xmlns:cv="clr-namespace:Bonsai.Vision;assembly=Bonsai.Vision"
                 xmlns:gli="clr-namespace:Bonsai.Shaders.Input;assembly=Bonsai.Shaders"
                 xmlns:gl="clr-namespace:Bonsai.Shaders;assembly=Bonsai.Shaders"
                 xmlns:rx="clr-namespace:Bonsai.Reactive;assembly=Bonsai.Core"
                 xmlns:harp="clr-namespace:Bonsai.Harp;assembly=Bonsai.Harp"
                 xmlns:p2="clr-namespace:OpenCV.Net;assembly=OpenCV.Net"
                 xmlns:p3="clr-namespace:Bonsai.Harp.Expander;assembly=Bonsai.Harp.Expander"
                 xmlns:glr="clr-namespace:Bonsai.Shaders.Rendering;assembly=Bonsai.Shaders.Rendering"
                 xmlns:res="clr-namespace:Bonsai.Resources;assembly=Bonsai.System"
                 xmlns:bv="clr-namespace:BonVision;assembly=BonVision"
                 xmlns:p4="clr-namespace:OpenTK;assembly=OpenTK"
                 xmlns:scr="clr-namespace:Bonsai.Scripting;assembly=Bonsai.Scripting"
                 xmlns:dsp="clr-namespace:Bonsai.Dsp;assembly=Bonsai.Dsp"
                 xmlns="https://bonsai-rx.org/2018/workflow">
  <Workflow>
    <Nodes>
      <Expression xsi:type="GroupWorkflow">
        <Name>__Init__</Name>
        <Workflow>
          <Nodes>
            <Expression xsi:type="BehaviorSubject" TypeArguments="sys:Tuple(sys:Single,sys:Single,sys:Single,sys:Single,sys:Single,sys:Single)">
              <Name>ArenaBounds</Name>
            </Expression>
            <Expression xsi:type="PublishSubject" TypeArguments="p1:VrElement">
              <Name>RodentPositionUpdate</Name>
            </Expression>
          </Nodes>
          <Edges />
        </Workflow>
      </Expression>
      <Expression xsi:type="GroupWorkflow">
        <Name>UserInterface</Name>
        <Workflow>
          <Nodes>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="wie:KeyDown">
                <wie:Filter>None</wie:Filter>
                <wie:SuppressRepetitions>false</wie:SuppressRepetitions>
              </Combinator>
            </Expression>
            <Expression xsi:type="ExternalizedMapping">
              <Property Name="X" Category="ResetPosition" />
              <Property Name="Y" Category="ResetPosition" />
            </Expression>
            <Expression xsi:type="Combinator">
              <Combinator xsi:type="cv:CreatePoint2f">
                <cv:X>0</cv:X>
                <cv:Y>0</cv:Y>
              </Combinator>
            </Expression>
            <Expression xsi:type="BehaviorSubject">
              <Name>ResetRodentPosition</Name>
            </Expression>
            <Expression xsi:type="GroupWorkflow">
              <Name>RodentUpdates</Name>
              <Workflow>
                <Nodes>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gli:Keyboard">
                      <gli:Index xsi:nil="true" />
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p1:KeySelectorScript">
                      <p1:KeyFilter>A</p1:KeyFilter>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gl:CreateVector3">
                      <gl:X>-1</gl:X>
                      <gl:Y>0</gl:Y>
                      <gl:Z>0</gl:Z>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p1:KeySelectorScript">
                      <p1:KeyFilter>D</p1:KeyFilter>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gl:CreateVector3">
                      <gl:X>1</gl:X>
                      <gl:Y>0</gl:Y>
                      <gl:Z>0</gl:Z>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p1:KeySelectorScript">
                      <p1:KeyFilter>W</p1:KeyFilter>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gl:CreateVector3">
                      <gl:X>0</gl:X>
                      <gl:Y>0</gl:Y>
                      <gl:Z>1</gl:Z>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p1:KeySelectorScript">
                      <p1:KeyFilter>S</p1:KeyFilter>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gl:CreateVector3">
                      <gl:X>0</gl:X>
                      <gl:Y>0</gl:Y>
                      <gl:Z>-1</gl:Z>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p1:KeySelectorScript">
                      <p1:KeyFilter>Z</p1:KeyFilter>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gl:CreateVector3">
                      <gl:X>0</gl:X>
                      <gl:Y>1</gl:Y>
                      <gl:Z>0</gl:Z>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p1:KeySelectorScript">
                      <p1:KeyFilter>X</p1:KeyFilter>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="gl:CreateVector3">
                      <gl:X>0</gl:X>
                      <gl:Y>-1</gl:Y>
                      <gl:Z>0</gl:Z>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="rx:Merge" />
                  </Expression>
                  <Expression xsi:type="InputMapping">
                    <PropertyMappings>
                      <Property Name="Position" Selector="it" />
                    </PropertyMappings>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p1:CreateVrElement">
                      <p1:Position>
                        <p1:X>0</p1:X>
                        <p1:Y>0</p1:Y>
                        <p1:Z>-1</p1:Z>
                      </p1:Position>
                      <p1:Orientation>
                        <p1:X>0</p1:X>
                        <p1:Y>0</p1:Y>
                        <p1:Z>0</p1:Z>
                      </p1:Orientation>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="MulticastSubject">
                    <Name>RodentPositionUpdate</Name>
                  </Expression>
                </Nodes>
                <Edges>
                  <Edge From="0" To="1" Label="Source1" />
                  <Edge From="0" To="3" Label="Source1" />
                  <Edge From="0" To="5" Label="Source1" />
                  <Edge From="0" To="7" Label="Source1" />
                  <Edge From="0" To="9" Label="Source1" />
                  <Edge From="0" To="11" Label="Source1" />
                  <Edge From="1" To="2" Label="Source1" />
                  <Edge From="2" To="13" Label="Source1" />
                  <Edge From="3" To="4" Label="Source1" />
                  <Edge From="4" To="13" Label="Source2" />
                  <Edge From="5" To="6" Label="Source1" />
                  <Edge From="6" To="13" Label="Source3" />
                  <Edge From="7" To="8" Label="Source1" />
                  <Edge From="8" To="13" Label="Source4" />
                  <Edge From="9" To="10" Label="Source1" />
                  <Edge From="10" To="13" Label="Source5" />
                  <Edge From="11" To="12" Label="Source1" />
                  <Edge From="12" To="13" Label="Source6" />
                  <Edge From="13" To="14" Label="Source1" />
                  <Edge From="14" To="15" Label="Source1" />
                  <Edge From="15" To="16" Label="Source1" />
                </Edges>
              </Workflow>
            </Expression>
            <Expression xsi:type="GroupWorkflow">
              <Name>ManualArmControl</Name>
              <Workflow>
                <Nodes>
                  <Expression xsi:type="GroupWorkflow">
                    <Name>Manual_Arm_Control</Name>
                    <Workflow>
                      <Nodes>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="gli:Keyboard">
                            <gli:Index xsi:nil="true" />
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="p1:KeySelectorScript">
                            <p1:KeyFilter>Up</p1:KeyFilter>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="cv:CreatePoint2f">
                            <cv:X>0</cv:X>
                            <cv:Y>1</cv:Y>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="p1:KeySelectorScript">
                            <p1:KeyFilter>Down</p1:KeyFilter>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="cv:CreatePoint2f">
                            <cv:X>0</cv:X>
                            <cv:Y>-1</cv:Y>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="p1:KeySelectorScript">
                            <p1:KeyFilter>Left</p1:KeyFilter>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="cv:CreatePoint2f">
                            <cv:X>-1</cv:X>
                            <cv:Y>0</cv:Y>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="p1:KeySelectorScript">
                            <p1:KeyFilter>Right</p1:KeyFilter>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="cv:CreatePoint2f">
                            <cv:X>1</cv:X>
                            <cv:Y>0</cv:Y>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="rx:Merge" />
                        </Expression>
                        <Expression xsi:type="ExternalizedMapping">
                          <Property Name="Value" DisplayName="Gain" />
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="FloatProperty">
                            <Value>0.01</Value>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="rx:Zip" />
                        </Expression>
                        <Expression xsi:type="Multiply" />
                        <Expression xsi:type="WorkflowOutput" />
                      </Nodes>
                      <Edges>
                        <Edge From="0" To="1" Label="Source1" />
                        <Edge From="0" To="3" Label="Source1" />
                        <Edge From="0" To="5" Label="Source1" />
                        <Edge From="0" To="7" Label="Source1" />
                        <Edge From="1" To="2" Label="Source1" />
                        <Edge From="2" To="9" Label="Source1" />
                        <Edge From="3" To="4" Label="Source1" />
                        <Edge From="4" To="9" Label="Source2" />
                        <Edge From="5" To="6" Label="Source1" />
                        <Edge From="6" To="9" Label="Source3" />
                        <Edge From="7" To="8" Label="Source1" />
                        <Edge From="8" To="9" Label="Source4" />
                        <Edge From="9" To="12" Label="Source1" />
                        <Edge From="9" To="11" Label="Source1" />
                        <Edge From="10" To="11" Label="Source2" />
                        <Edge From="11" To="12" Label="Source2" />
                        <Edge From="12" To="13" Label="Source1" />
                        <Edge From="13" To="14" Label="Source1" />
                      </Edges>
                    </Workflow>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="rx:GateInterval">
                      <rx:Interval>PT0.01S</rx:Interval>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="MulticastSubject">
                    <Name>UpdateArmCartPosition</Name>
                  </Expression>
                  <Expression xsi:type="WorkflowOutput" />
                </Nodes>
                <Edges>
                  <Edge From="0" To="1" Label="Source1" />
                  <Edge From="1" To="2" Label="Source1" />
                  <Edge From="2" To="3" Label="Source1" />
                </Edges>
              </Workflow>
            </Expression>
          </Nodes>
          <Edges>
            <Edge From="0" To="2" Label="Source1" />
            <Edge From="1" To="2" Label="Source2" />
            <Edge From="2" To="3" Label="Source1" />
          </Edges>
        </Workflow>
      </Expression>
      <Expression xsi:type="GroupWorkflow">
        <Name>Hardware</Name>
        <Workflow>
          <Nodes>
            <Expression xsi:type="GroupWorkflow">
              <Name>SWC_Arm</Name>
              <Workflow>
                <Nodes>
                  <Expression xsi:type="Disable">
                    <Builder xsi:type="ExternalizedMapping">
                      <Property Name="PortName" />
                    </Builder>
                  </Expression>
                  <Expression xsi:type="BehaviorSubject" TypeArguments="harp:HarpMessage">
                    <Name>ArmCommands</Name>
                  </Expression>
                  <Expression xsi:type="Disable">
                    <Builder xsi:type="Combinator">
                      <Combinator xsi:type="harp:Device">
                        <harp:DeviceState>Active</harp:DeviceState>
                        <harp:DumpRegisters>true</harp:DumpRegisters>
                        <harp:LedState>On</harp:LedState>
                        <harp:VisualIndicators>On</harp:VisualIndicators>
                        <harp:Heartbeat>Disable</harp:Heartbeat>
                        <harp:IgnoreErrors>true</harp:IgnoreErrors>
                        <harp:PortName>COM5</harp:PortName>
                      </Combinator>
                    </Builder>
                  </Expression>
                  <Expression xsi:type="PublishSubject">
                    <Name>ArmEvents</Name>
                  </Expression>
                  <Expression xsi:type="SubscribeSubject">
                    <Name>ArmEvents</Name>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="harp:FilterMessage">
                      <harp:Address>33</harp:Address>
                      <harp:MessageType>Event</harp:MessageType>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="PublishSubject">
                    <Name>ArmEncoderPosition</Name>
                  </Expression>
                  <Expression xsi:type="BehaviorSubject" TypeArguments="p2:Point2f">
                    <Name>ReferenceArmPosition</Name>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p1:CreateArmMessage">
                      <p1:Address>34</p1:Address>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="MulticastSubject">
                    <Name>ArmCommands</Name>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="cv:CreatePoint2f">
                      <cv:X>0.15</cv:X>
                      <cv:Y>0.3</cv:Y>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="BehaviorSubject">
                    <Name>RadialLimits</Name>
                  </Expression>
                  <Expression xsi:type="Combinator">
                    <Combinator xsi:type="p1:CreateArmMessage">
                      <p1:Address>35</p1:Address>
                    </Combinator>
                  </Expression>
                  <Expression xsi:type="MulticastSubject">
                    <Name>ArmCommands</Name>
                  </Expression>
                  <Expression xsi:type="GroupWorkflow">
                    <Name>SetAngleMode</Name>
                    <Workflow>
                      <Nodes>
                        <Expression xsi:type="ExternalizedMapping">
                          <Property Name="Value" DisplayName="AngleMode" />
                        </Expression>
                        <Expression xsi:type="Combinator">
                          <Combinator xsi:type="IntProperty">
                            <Value>0</Value>
                          </Combinator>
                        </Expression>
                        <Expression xsi:type="harp:Format">
                          <harp:MessageType>Write</harp:MessageType>
                          <harp:Address>36</harp:Address>
                          <harp:PayloadType>S8</harp:PayloadType>
                        </Expression>
                        <Expression xsi:type="WorkflowOutput" />
                      </Nodes>
                      <Edges>
                        <Edge From="0" To="1" Label="Source1" />
                        <Edge From="1" To="2" Label="Source1" />
                        <Edge From="2" To="3" Label="Source1" />
                      </Edges>
                    </Workflow>
                  </Expression>
                  <Expression xsi:type="MulticastSubject">
                    <Name>ArmCommands</Name>
                  </Expression>
                </Nodes>
                <Edges>
                  <Edge From="0" To="2" Label="Source2" />
                  <Edge From="1" To="2" Label="Source1" />
                  <Edge From="2" To="3" Label="Source1" />
                  <Edge From="4" To="5" Label="Source1" />
                  <Edge From="5" To="6" Label="Source1" />
                  <Edge From="7" To="8" Label="Source1" />
                  <Edge From="8" To="9" Label="Source1" />
                  <Edge From="10" To="11" Label="Source1" />
                  <Edge From="11" To="12" Label="Source1" />
                  <Edge From="12" To="13" Label="Source1" />
                  <Edge From="14" To="15" Label="Source1" />
                </Edges>
              </Workflow>
            </Expression>
            <Expression xsi:type="Disable">
              <Builder xsi:type="GroupWorkflow">
                <Name>HARP_Expander</Name>
                <Workflow>
                  <Nodes>
                    <Expression xsi:type="ExternalizedMapping">
                      <Property Name="PortName" />
                    </Expression>
                    <Expression xsi:type="GroupWorkflow">
                      <Name>CreateExpander</Name>
                      <Workflow>
                        <Nodes>
                          <Expression xsi:type="BehaviorSubject" TypeArguments="harp:HarpMessage">
                            <Name>ExpanderCommands</Name>
                          </Expression>
                          <Expression xsi:type="ExternalizedMapping">
                            <Property Name="PortName" />
                          </Expression>
                          <Expression xsi:type="Combinator">
                            <Combinator xsi:type="harp:Device">
                              <harp:DeviceState>Active</harp:DeviceState>
                              <harp:DumpRegisters>true</harp:DumpRegisters>
                              <harp:LedState>On</harp:LedState>
                              <harp:VisualIndicators>On</harp:VisualIndicators>
                              <harp:Heartbeat>Disable</harp:Heartbeat>
                              <harp:IgnoreErrors>false</harp:IgnoreErrors>
                              <harp:PortName>COMx</harp:PortName>
                            </Combinator>
                          </Expression>
                          <Expression xsi:type="PublishSubject">
                            <Name>ExpanderEvents</Name>
                          </Expression>
                          <Expression xsi:type="WorkflowOutput" />
                        </Nodes>
                        <Edges>
                          <Edge From="0" To="2" Label="Source1" />
                          <Edge From="1" To="2" Label="Source2" />
                          <Edge From="2" To="3" Label="Source1" />
                          <Edge From="3" To="4" Label="Source1" />
                        </Edges>
                      </Workflow>
                    </Expression>
                    <Expression xsi:type="ExternalizedMapping">
                      <Property Name="CameraTriggerFrequency" />
                      <Property Name="StartDelay" />
                    </Expression>
                    <Expression xsi:type="GroupWorkflow">
                      <Name>CameraController</Name>
                      <Workflow>
                        <Nodes>
                          <Expression xsi:type="ExternalizedMapping">
                            <Property Name="Value" DisplayName="CameraTriggerFrequency" />
                          </Expression>
                          <Expression xsi:type="Combinator">
                            <Combinator xsi:type="IntProperty">
                              <Value>50</Value>
                            </Combinator>
                          </Expression>
                          <Expression xsi:type="ReplaySubject">
                            <Name>CameraTriggerFrequency</Name>
                            <BufferSize xsi:nil="true" />
                          </Expression>
                          <Expression xsi:type="ExternalizedMapping">
                            <Property Name="StartDelay" />
                          </Expression>
                          <Expression xsi:type="GroupWorkflow">
                            <Name>CameraController</Name>
                            <Workflow>
                              <Nodes>
                                <Expression xsi:type="PublishSubject" TypeArguments="sys:Object">
                                  <Name>StartCameras</Name>
                                </Expression>
                                <Expression xsi:type="Unit" />
                                <Expression xsi:type="PublishSubject" TypeArguments="sys:Object">
                                  <Name>StopCameras</Name>
                                </Expression>
                                <Expression xsi:type="Unit" />
                                <Expression xsi:type="ExternalizedMapping">
                                  <Property Name="DueTime" DisplayName="StartDelay" />
                                </Expression>
                                <Expression xsi:type="Combinator">
                                  <Combinator xsi:type="rx:Timer">
                                    <rx:DueTime>PT1S</rx:DueTime>
                                    <rx:Period>PT0S</rx:Period>
                                  </Combinator>
                                </Expression>
                                <Expression xsi:type="Unit" />
                                <Expression xsi:type="Combinator">
                                  <Combinator xsi:type="rx:Merge" />
                                </Expression>
                                <Expression xsi:type="Combinator">
                                  <Combinator xsi:type="p3:PwmOutput">
                                    <p3:Command>Start</p3:Command>
                                    <p3:Mask>Pwm1</p3:Mask>
                                  </Combinator>
                                </Expression>
                                <Expression xsi:type="Unit" />
                                <Expression xsi:type="Combinator">
                                  <Combinator xsi:type="rx:Merge" />
                                </Expression>
                                <Expression xsi:type="Combinator">
                                  <Combinator xsi:type="p3:PwmOutput">
                                    <p3:Command>Stop</p3:Command>
                                    <p3:Mask>Pwm1</p3:Mask>
                                  </Combinator>
                                </Expression>
                                <Expression xsi:type="Combinator">
                                  <Combinator xsi:type="IntProperty">
                                    <Value>0</Value>
                                  </Combinator>
                                </Expression>
                                <Expression xsi:type="SubscribeSubject">
                                  <Name>CameraTriggerFrequency</Name>
                                </Expression>
                                <Expression xsi:type="PropertyMapping">
                                  <PropertyMappings>
                                    <Property Name="Frequency" />
                                  </PropertyMappings>
                                </Expression>
                                <Expression xsi:type="Combinator">
                                  <Combinator xsi:type="p3:ConfigurePwm">
                                    <p3:Mask>PwmOutput1</p3:Mask>
                                    <p3:Frequency>100</p3:Frequency>
                                    <p3:DutyCycle>50</p3:DutyCycle>
                                    <p3:Count>0</p3:Count>
                                  </Combinator>
                                </Expression>
                                <Expression xsi:type="Combinator">
                                  <Combinator xsi:type="rx:Merge" />
                                </Expression>
                                <Expression xsi:type="MulticastSubject">
                                  <Name>ExpanderCommands</Name>
                                </Expression>
                                <Expression xsi:type="WorkflowOutput" />
                              </Nodes>
                              <Edges>
                                <Edge From="0" To="1" Label="Source1" />
                                <Edge From="1" To="7" Label="Source2" />
                                <Edge From="2" To="3" Label="Source1" />
                                <Edge From="3" To="10" Label="Source2" />
                                <Edge From="4" To="5" Label="Source1" />
                                <Edge From="5" To="6" Label="Source1" />
                                <Edge From="6" To="7" Label="Source1" />
                                <Edge From="7" To="8" Label="Source1" />
                                <Edge From="8" To="16" Label="Source3" />
                                <Edge From="9" To="10" Label="Source1" />
                                <Edge From="10" To="11" Label="Source1" />
                                <Edge From="11" To="16" Label="Source1" />
                                <Edge From="12" To="15" Label="Source1" />
                                <Edge From="13" To="14" Label="Source1" />
                                <Edge From="14" To="15" Label="Source2" />
                                <Edge From="15" To="16" Label="Source2" />
                                <Edge From="16" To="17" Label="Source1" />
                                <Edge From="17" To="18" Label="Source1" />
                              </Edges>
                            </Workflow>
                          </Expression>
                          <Expression xsi:type="SubscribeSubject">
                            <Name>ExpanderEvents</Name>
                          </Expression>
                          <Expression xsi:type="p3:PwmRise">
                            <p3:IncludeTimestamp>true</p3:IncludeTimestamp>
                          </Expression>
                          <Expression xsi:type="Condition">
                            <Name>WhereChannel</Name>
                            <Workflow>
                              <Nodes>
                                <Expression xsi:type="WorkflowInput">
                                  <Name>Source1</Name>
                                </Expression>
                                <Expression xsi:type="MemberSelector">
                                  <Selector>Value</Selector>
                                </Expression>
                                <Expression xsi:type="ExternalizedMapping">
